#!/bin/sh
#
# Usage: atom-package-list
#
# Lists the currently installed atom packages
# located at ~/.dotfiles/atom/.atom+packages.txt.symlink
#
# You can generate a new list based on currently installed
# packages via atom-package-backup

set -e

./atom-package-list > ~/.dotfiles/atom/.atom+packages.txt.symlink

# apm list --installed --bare | grep '^[^@]\+' -o
