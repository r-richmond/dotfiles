{
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "files.associations": {
    "*.json": "jsonc",
    // this looks best
    "*.brewfile": "pip-requirements"
  },
  "gitlens.blame.dateFormat": "YYYY-MM-DD",
  "gitlens.currentLine.dateFormat": "YYYY-MM-DD",
  "gitlens.defaultDateFormat": "YYYY-MM-DD hh:mm:ss",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.dateFormat": "YYYY-MM-DD",
  "path-intellisense.showHiddenFiles": true,
  "prettier.printWidth": 120,
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.organizeImports": true
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "python.defaultInterpreterPath": "python3",
  "python.testing.unittestEnabled": true,
  "python.pythonPath": "python3",
  "python.languageServer": "Pylance",
  "python.experiments.enabled": false,
  "python.jediEnabled": false,
  "python.interpreter.infoVisibility": "always",
  "python.analysis.memory.keepLibraryAst": true,
  "python.analysis.memory.keepLibraryLocalVariables": true,
  "ruff.interpreter": ["python3"],
  "ruff.path": ["~/.local/bin/ruff"],
  "shellcheck.customArgs": ["-e SC1071"],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "workbench.colorTheme": "An Old Hope Classic",
  "workbench.iconTheme": "material-icon-theme",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "sqltools.highlightQuery": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "github.gitAuthentication": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.codeLens.enabled": false,
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.guides.bracketPairs": true,
  "telemetry.telemetryLevel": "off",
  "editor.renderWhitespace": "none",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "terminal.integrated.shellIntegration.enabled": false,
  "workbench.startupEditor": "none",
  "terminal.explorerKind": "external",
  "terminal.integrated.shellIntegration.history": 10000,
  "terminal.integrated.scrollback": 100000,
  "editor.fontFamily": "'MesloLGS NF', 'Hack Nerd Font Mono', 'Hack Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "editor.codeLensFontFamily": "'Hack Nerd Font Mono', 'Hack Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "diffEditor.experimental.useVersion2": true,
  "github.copilot.editor.enableAutoCompletions": false
}
